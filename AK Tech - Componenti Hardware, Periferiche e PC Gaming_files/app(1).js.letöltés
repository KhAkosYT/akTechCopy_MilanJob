var app = new vue__WEBPACK_IMPORTED_MODULE_0___default.a({
  el: '#header',
  data: function data() {
    return {
      menuOpen: true,
      megaMenuOpen: false,
      selectedMega: null
    };
  },
  created: function created() {
    console.log('webdevemilio nuovo');
  },
  methods: {
    changeLabel: function changeLabel(menuid) {
      //console.log(menuid);

      //RESET
      this.resetMegas();
      this.selectedMega = menuid;
      //console.log("Selected mega: ", this.selectedMega)

      var isMobileDevice = navigator.userAgent.match(/iPad|iPhone|iPod/i) != null || screen.width <= 480;
      if (isMobileDevice) {
        //console.log("Caso  mobilexx");
        var myDiv = document.getElementById('flyoutmenu');
        myDiv.scrollTop = 0;
        document.getElementById('mega-' + menuid).style.display = "block";
        document.getElementById('mega-' + menuid).style.width = "100%";
        this.megaMenuOpen = true;
      } else {
        //this.megaMenuOpen = true;
        document.getElementById('mega-' + menuid).style.display = "block";
        //console.log("Caso non mobile");
      }
    },
    backMenu: function backMenu() {
      this.megaMenuOpen = false;
      //console.log("backMenu");
    },
    resetMegas: function resetMegas() {
      var megas = document.querySelectorAll('.mega-menu');
      for (var i = 0; i < megas.length; i++) {
        megas[i].style.display = "none";
      }
    },
    openMenu: function openMenu() {
      this.menuOpen = !this.menuOpen;
      this.resetMegas();
      var isMobileDevice = navigator.userAgent.match(/iPad|iPhone|iPod/i) != null || screen.width <= 480;
      if (this.menuOpen) {
        if (isMobileDevice) {
          document.getElementById('flyoutmenu').style.left = "-100%";
          document.getElementById('flyoutmenu').style.top = "64px";
          document.getElementById('nav-backdrop').style.display = "none";
        } else {
          document.getElementById('flyoutmenu').style.left = "-100%";
          document.getElementById('flyoutmenu').style.top = "60px";
          document.getElementById('nav-backdrop').style.display = "none";
        }
      } else {
        if (isMobileDevice) {
          document.getElementById('flyoutmenu').style.left = "0px";
          document.getElementById('flyoutmenu').style.top = "64px";
          document.getElementById('nav-backdrop').style.display = "block";
        } else {
          document.getElementById('flyoutmenu').style.left = "0px";
          document.getElementById('flyoutmenu').style.top = "104px";
          document.getElementById('nav-backdrop').style.display = "block";
        }
      }
    }
  }
});

