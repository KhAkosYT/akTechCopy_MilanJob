/**
 * Popup cookie
 */
document.addEventListener("DOMContentLoaded", function () {
  if (document.getElementById("custom-ak-button")) {
    var akPopup = document.getElementsByClassName("pum");
    Array.from(akPopup).forEach(function (el) {
      //Get the id of popup
      var elementId = el.id;
      //console.log(elementId);
      if (js_cookie__WEBPACK_IMPORTED_MODULE_3___default.a.get('ak-' + elementId)) {
        //The cookie are valid
        //console.log('Recent user for '+elementId);
        el.removeAttribute("style");
        el.style.display = "none";
      } else {
        //New user or cookie was expire
        //console.log('New user or cookie expired for '+elementId);
        js_cookie__WEBPACK_IMPORTED_MODULE_3___default.a.set('ak-' + elementId, 'visited_' + elementId, {
          expires: 1
        });
      }
    });
  }
});

//Back to top
$(function () {
  $(window).scroll(function () {
    if ($(this).scrollTop() != 0) {
      $('#toTop').fadeIn();
    } else {
      $('#toTop').fadeOut();
    }
  });
  $('#toTop').click(function () {
    $('body,html').animate({
      scrollTop: 0
    }, 800);
  });
});
if (isMobile()) {
  if (document.getElementById('place_order_div') || document.getElementById('place_order')) {
    $(window).on('resize scroll', function () {
      if ($('#place_order_div').isInViewport()) {
        $('#cta_place').removeClass('fixed-checkout ').addClass('block-checkout z-1');
        //console.log('in viewport')
      } else {
        $('#cta_place').removeClass('block-checkout').addClass('fixed-checkout');
        //console.log('out of viewport')
      }
      if ($('#footer').isInViewport() && $('#place_order')) {
        $('#place_order').removeClass('fixed-checkout').addClass('block-checkout');
      } else {
        $('#place_order').removeClass('block-checkout').addClass('fixed-checkout');
      }
    });
  }
}

/**
 * Countdown Sale
 */
jQuery(document).ready(function ($) {
  function updateCountdownTimer() {
    var now = new Date().getTime();
    $('.sale-countdown').each(function () {
      var endTime = parseInt($(this).data('endtime')) * 1000;
      var startTime = parseInt($(this).data('starttime')) * 1000;
      var distance;
      if (now > startTime && now < endTime) {
        distance = endTime - now;
      } else if (now < startTime) {
        $(this).hide();
        return;
      } else if (now >= endTime) {
        $(this).hide();
        return;
      }
      var days = Math.floor(distance / (1000 * 60 * 60 * 24));
      var hours = Math.floor(distance % (1000 * 60 * 60 * 24) / (1000 * 60 * 60));
      var minutes = Math.floor(distance % (1000 * 60 * 60) / (1000 * 60));
      var seconds = Math.floor(distance % (1000 * 60) / 1000);
      $(this).find('.days').text(days);
      $(this).find('.hours').text(hours);
      $(this).find('.minutes').text(minutes);
      $(this).find('.seconds').text(seconds);
    });
  }
  setInterval(updateCountdownTimer, 1000);
  updateCountdownTimer();
});
