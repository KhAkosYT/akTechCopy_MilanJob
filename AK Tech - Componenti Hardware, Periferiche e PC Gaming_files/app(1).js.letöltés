jQuery(".carousel").each(function (index, element) {
  var $this = jQuery(this);
  $this.addClass("instance-" + index);
  $this.next(".swiper-button-prev-carousel").addClass("swiper-button-prev-carousel-" + index);
  $this.next().next(".swiper-button-next-carousel").addClass("swiper-button-next-carousel-" + index);

  //Get number slides param for desk
  if ($this.data().slides_number) {
    var slidesNumberDesk = $this.data().slides_number;
  } else {
    var slidesNumberDesk = 5;
  }

  //Get number slides param for desk2
  if ($this.data().slides_number2) {
    var slidesNumberDesk2 = $this.data().slides_number2;
  } else {
    var slidesNumberDesk2 = 4;
  }

  //Get number slides param for desk2
  if ($this.data().slides_number3) {
    var slidesNumberDesk3 = $this.data().slides_number3;
  } else {
    var slidesNumberDesk3 = 3;
  }

  //Get number slides param for tablet
  if ($this.data().slides_number_tablet) {
    var slidesNumberTablet = $this.data().slides_number_tablet;
  } else {
    var slidesNumberTablet = 2;
  }

  //Get number slides param for phone
  if ($this.data().slides_number_phone) {
    var slidesNumberPhone = $this.data().slides_number_phone;
  } else {
    var slidesNumberPhone = 1;
  }

  // Determina se attivare il loop
  var shouldLoop = !$this.hasClass('carousel-countdown');
  var carousel = new swiper__WEBPACK_IMPORTED_MODULE_1__["default"](".instance-" + index, {
    observeParents: true,
    observeSlideChildren: true,
    observer: true,
    autoplay: {
      delay: 3000
    },
    loop: shouldLoop,
    slidesPerView: 4,
    spaceBetween: 5,
    autoHeight: true,
    breakpoints: {
      // when window width is >= 320px
      320: {
        slidesPerView: slidesNumberPhone,
        spaceBetween: 20
      },
      // when window width is >= 480px
      480: {
        slidesPerView: slidesNumberTablet,
        spaceBetween: 20
      },
      // when window width is >= 1024px
      1024: {
        slidesPerView: slidesNumberDesk3,
        spaceBetween: 20
      },
      1500: {
        slidesPerView: slidesNumberDesk2,
        spaceBetween: 20
      },
      1600: {
        slidesPerView: slidesNumberDesk,
        spaceBetween: 20
      }
    },
    navigation: {
      nextEl: ".swiper-button-next-carousel-" + index,
      prevEl: ".swiper-button-prev-carousel-" + index
    },
    on: {
      init: function init() {
        //console.log('swiper initialized');
        var swiperLists = document.querySelectorAll(".instance-" + index);
        //console.log("SWIPERLIST", swiperLists);
        for (var i = 0; i < swiperLists.length; i++) {
          //console.log(swiperLists[i]);
          swiperLists[i].classList.remove('hidden');
          swiperLists[i].classList.add("animate-fadeIn");
        }
        var swiperArrowPrev = document.querySelectorAll(".swiper-button-prev");
        for (var _i = 0; _i < swiperArrowPrev.length; _i++) {
          swiperArrowPrev[_i].classList.remove('hidden');
          swiperArrowPrev[_i].classList.add("animate-fadeIn");
        }
        var swiperArrowNext = document.querySelectorAll(".swiper-button-next");
        for (var _i2 = 0; _i2 < swiperArrowNext.length; _i2++) {
          swiperArrowNext[_i2].classList.remove('hidden');
          swiperArrowNext[_i2].classList.add("animate-fadeIn");
        }
      }
    }
  });
});

/*
* Animations on viewport
*/
var callback = function callback(entries) {
  entries.forEach(function (entry) {
    //console.log(entry);

    if (entry.isIntersecting) {
      entry.target.classList.add("animate-fadeIn");
    } else {
      entry.target.classList.remove("animate-fadeIn");
    }
  });
};
var observer = new IntersectionObserver(callback);
var targets = document.querySelectorAll(".js-show-on-scroll");
targets.forEach(function (target) {
  target.classList.add("opacity-0");
  observer.observe(target);
});

