/**
 * On Scroll fixed only if footer is not in viewport.
 */
function isMobile() {
  var toMatch = [/Android/i, /webOS/i, /iPhone/i, /iPad/i, /iPod/i, /BlackBerry/i, /Windows Phone/i];
  return toMatch.some(function (toMatchItem) {
    return navigator.userAgent.match(toMatchItem);
  });
}
if (isMobile() && $('#footer') || isMobile() && $('#place_order_div')) {
  $.fn.isInViewport = function () {
    var elementTop = $(this).offset().top;
    var elementBottom = elementTop + $(this).outerHeight();
    var viewportTop = $(window).scrollTop();
    var viewportBottom = viewportTop + $(window).height();
    return elementBottom > viewportTop && elementTop < viewportBottom;
  };
}

/**
 * Filter engine
 */
var mobileFilterProduct = document.getElementById('mobile-filter-open');
var showFilter = document.getElementById('sidebar-shop');
var closeFilter = document.getElementById('closeFilterIcon');
var searchButton = document.getElementById('search-button');
if (mobileFilterProduct) {
  mobileFilterProduct.addEventListener("click", toggleFilter);
}
if (closeFilter || searchButton) {
  closeFilter.addEventListener("click", closeFilterProduct);
  searchButton.addEventListener("click", closeFilterProduct);
}
function toggleFilter() {
  //console.log('Clicked toggle');

  if (showFilter.classList.contains("filter-hidden")) {
    showFilter.classList.remove("filter-hidden");
    showFilter.classList.add("filter-show");
    document.body.style.overflow = "hidden";
  } else {
    showFilter.classList.add("filter-hidden");
    showFilter.classList.remove("filter-show");
  }
}
function closeFilterProduct() {
  //console.log('Clicked closed!')
  showFilter.classList.add("filter-hidden");
  showFilter.classList.remove("filter-show");
  document.body.style.overflow = "scroll";
}

